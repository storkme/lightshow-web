{
  "name": "wallboard-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cleanup": "rimraf build dist",
    "explorer": "source-map-explorer ./dist/main.bundle.js ./dist/main.bundle.js.map",
    "ngc": "ngc",
    "build:jit": "npm run cleanup && webpack --config webpack.jit.config.js -p",
    "build": "npm run cleanup && npm run ngc && webpack --config webpack.aot.config.js -p",
    "serve-watch:aot": "npm run cleanup && npm run ngc && concurrently -r \"webpack-dev-server --config webpack.aot.config.js\" \"node ./bin/ngc-watch.js\"",
    "serve-watch:jit": "npm run cleanup && webpack-dev-server --config webpack.jit.config.js",
    "serve:dist": "http-server ./dist -a 0.0.0.0 -p 9001",
    "test:once": "karma start karma.conf.js --single-run"
  },
  "author": "alex.storkey@eckoh.com",
  "license": "ISC",
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.4",
    "@angular/platform-server": "^2.4.4",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.4",
    "@types/sinon": "^1.16.35",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "http-server": "^0.9.0",
    "jasmine": "^2.5.3",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^2.1.7",
    "karma-webpack": "^2.0.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "source-map-explorer": "^1.3.3",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.0.10",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@angular/http": "^2.4.4",
    "@angular/platform-browser": "^2.4.4",
    "@angular/platform-browser-dynamic": "^2.4.4",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.6"
  }
}
